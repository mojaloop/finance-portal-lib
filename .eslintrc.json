{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "__set__",
          "__get__"
        ]
      }
    ]
  }
}
